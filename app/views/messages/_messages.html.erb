

<% @all_messages.each do |message| %>
    <p class="customer-message">From: <%=message.from_number %> <%=message.body%></p>
    <% if message.replies.any? %>
      <% message.replies.each do |reply| %>
        <p class="food-cart-message"> <%= reply.body %> </p>
      <% end %>
    <% end %>
  <% end %>
<% end %>
